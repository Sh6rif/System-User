    <main class="flex-column flex-sm-row d-flex flex-nowrap">
      <div
        id="sidebar"
        class="position-relative d-flex flex-row flex-sm-column flex-shrink-0 p-3 text-bg-dark sidebar"
      >
        <span class="position-absolute">
          <button class="btn" onclick="SideBar()">
            <i
              style="cursor: pointer"
              id="arrowCircle"
              class="bi bi-arrow-left-circle text-white"
            ></i>
          </button>
        </span>
        <a
          id="lightningSide"
          class="lightningSide align-items-center mb-3 mb-md-0 text-white text-decoration-none"
        >
          <i
            id="lightning"
            class="lightning bi bi-lightning-charge bi-small"
          ></i>
          <span class="sidebar-text fs-4">Sidebar</span>
        </a>
        <hr />
        <ul class="sideContent nav nav-pills flex-column mb-auto">
          <li class="nav-item">
            <a
              style="cursor: pointer"
              onclick="Home()"
              id="homeSide"
              class="nav-link text-white active"
              aria-current="page"
            >
              <svg class="bi pe-none me-2" width="16" height="16">
                <use xlink:href="#home" />
              </svg>
              <span class="sidebar-text">Home</span>
            </a>
          </li>
          <li>
            <a
              id="addCouresSide"
              style="cursor: pointer"
              class="nav-link text-white"
              onclick="Add()"
            >
              <i class="bi bi-mortarboard pe-none me-2"></i>
              <span class="sidebar-text">Add Course</span>
            </a>
          </li>
        </ul>
        <hr />
        <div class="dropdown">
          <a
            href="#"
            class="d-flex align-items-center text-white text-decoration-none dropdown-toggle"
            data-bs-toggle="dropdown"
            aria-expanded="false"
          >
            <img
              src="./img/1186714.jpg"
              alt=""
              width="32"
              height="32"
              class="object-fit-cover rounded-circle me-2"
            />
            <strong class="sidebar-text">Sherif</strong>
          </a>
          <ul class="dropdown-menu dropdown-menu-dark text-small shadow">
            <li><a class="dropdown-item" href="#">Profile</a></li>
            <li><hr class="dropdown-divider" /></li>
            <li><a class="dropdown-item" href="#">Sign out</a></li>
          </ul>
        </div>
      </div>
      <div class="h-1 w-100 b-example-divider b-example-vr"></div>
      <section class="w-100 overflow-y-auto">
        <nav class="navbar bg-body-tertiary">
          <div class="container-fluid flex-column flex-md-row">
            <div>
              <i
                id="backArrow"
                style="cursor: pointer"
                class="d-none bi-arrow-left"
                onclick="Back()"
              ></i>
              <a id="headTitle" class="navbar-brand fw-medium fs-4">Courses</a>
            </div>
            <form class="d-flex" role="search">
              <input
                id="search"
                class="form-control me-2"
                type="search"
                placeholder="Search"
                aria-label="Search"
              />
              <button class="btn btn-outline-success" type="submit">
                Search
              </button>
            </form>
          </div>
        </nav>
        <div class="table-responsive">
          <table
            id="infoTable"
            style="width: 98%"
            class="table table-striped table-bordered mt-5 mx-auto text-center"
          >
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Code</th>
                <th scope="col">Name</th>
                <th scope="col">Category</th>
                <th scope="col">Lesson</th>
                <th scope="col">Last Update</th>
                <th scope="col">Action</th>
              </tr>
            </thead>
            <tbody>
              <% arr.forEach((item,index) => { %>
              <tr>
                <th scope="row"><%= index + 1 %></th>
                <td><%= item.code %></td>
                <td><%= item.name %></td>
                <td><%= item.category %></td>
                <td><%= item.lesson %></td>
                <th>... days</th>
                <td>
                  <button
                    onclick="View('<%= item.code %>')"
                    data-bs-toggle="tooltip"
                    data-bs-title="View Details"
                    data-bs-placement="top"
                    class="btn btn-primary btn-sm"
                  >
                    <i class="bi bi-eye"></i>
                  </button>
                  <button
                    onclick="Edit()"
                    data-bs-toggle="tooltip"
                    data-bs-title="Edit Course"
                    data-bs-placement="top"
                    class="btn btn-info btn-sm"
                  >
                    <i class="bi bi-pencil"></i>
                  </button>
                  <button
                    data-bs-toggle="modal"
                    data-bs-target="#exampleModal"
                    data-bs-toggle="tooltip"
                    data-bs-title="Delete Course"
                    data-bs-placement="top"
                    class="btn btn-danger btn-sm"
                  >
                    <i class="bi bi-trash3"></i>
                  </button>
                </td>
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
        <form
          action="/"
          id="infoForm"
          class="d-none row mt-2 mx-2 g-3"
          method="post"
          enctype="multipart/form-data"
        >
          <div class="col-md-6">
            <label for="inputCode" class="form-label">Code:</label>
            <input
              required
              type="number"
              name="code"
              min="0"
              class="form-control"
              id="inputCode"
            />
          </div>
          <div class="col-md-6">
            <label for="inputName4" class="form-label">Name:</label>
            <input
              required
              type="text"
              name="name"
              class="form-control"
              id="inputName4"
            />
          </div>
          <div class="col-md-6">
            <label for="inputCategory" class="form-label">Category:</label>
            <select name="category" id="inputCategory" class="form-select">
              <option selected>Adult</option>
              <option>Kids</option>
            </select>
          </div>
          <div class="col-md-6">
            <label for="inputLesson" class="form-label">Lesson:</label>
            <input
              required
              name="lesson"
              type="number"
              min="1"
              class="form-control"
              id="inputLesson"
            />
          </div>
          <div class="col-md-6 form-floating">
            <textarea
              required
              name="description"
              class="form-control"
              placeholder="Course Description"
              id="floatingTextareaDescription"
              style="height: 100px"
            ></textarea>
            <label for="floatingTextareaDescription">Course Description:</label>
          </div>
          <div class="col-md-6 form-floating">
            <textarea
              required
              name="lessonContent"
              class="form-control"
              placeholder="Lesson Content"
              id="floatingTextareaLesson"
              style="height: 100px"
            ></textarea>
            <label for="floatingTextareaLesson">Lesson Content:</label>
          </div>
          <div class="col-md-4">
            <label for="formFileThumbnails" class="form-label"
              >Thumbnails:</label
            >
            <input
              required
              name="thumbnails"
              class="form-control"
              type="file"
              id="formFileThumbnails"
              accept="image/*"
            />
          </div>
          <div class="col-md-4">
            <label for="formFileMultipleIntro" class="form-label"
              >Course Intro:</label
            >
            <input
              required
              name="courseIntro"
              class="form-control"
              type="file"
              id="formFileMultipleIntro"
              accept="video/*"
            />
          </div>
          <div class="col-md-4">
            <label for="formFileMultiplePlayList" class="form-label"
              >Course PlayList:</label
            >
            <input
              required
              name="coursePlayList"
              class="form-control"
              type="file"
              id="formFileMultiplePlayList"
              accept="video/*"
              multiple
            />
          </div>
          <div class="col-md-6">
            <p class="form-label">Duration:</p>
            <div class="d-flex align-items-baseline">
              <input
                name="hours"
                type="number"
                min="0"
                class="form-control me-2"
                id="inputHours"
                value="0"
              />
              <label for="inputHours" class="form-label">Hours</label>
              <input
                name="minutes"
                type="number"
                min="0"
                max="59"
                value="0"
                class="form-control mx-2"
                id="inputMinutes"
              />
              <label for="inputMinutes" class="form-label">Minutes</label>
              <input
                name="seconds"
                type="number"
                min="0"
                max="59"
                value="0"
                class="form-control mx-2"
                id="inputSeconds"
              />
              <label for="inputSeconds" class="form-label">Seconds</label>
            </div>
          </div>
          <div class="col-12">
            <button id="fromSubmit" type="submit" class="btn btn-primary">
              Add
            </button>
          </div>
        </form>
        <table
          id="viewTable"
          style="width: 98%"
          class="table table-striped table-bordered mt-5 mx-auto d-none"
        >
          <tbody>
            <tr>
              <th scope="row">Code</th>
              <td id="tdCode"></td>
            </tr>
            <tr>
              <th scope="row">Name</th>
              <td id="tdName"></td>
            </tr>
            <tr>
              <th scope="row">Category</th>
              <td id="tdCg"></td>
            </tr>
            <tr>
              <th scope="row">Lesson</th>
              <td id="tdLsn"></td>
            </tr>
            <tr>
              <th scope="row">Duration</th>
              <td id="tdDur"></td>
            </tr>
            <tr>
              <th scope="row">Thumbnails</th>
              <td>
                <a
                  target="”_blank”"
                  id="thumbLink"
                  href=""
                  class="link-info link-offset-2 link-underline-opacity-25 link-underline-opacity-100-hover"
                  >Thumbnails</a
                >
              </td>
            </tr>
            <tr>
              <th scope="row">Intro</th>
              <td>
                <a
                  target="”_blank”"
                  id="introLink"
                  href=""
                  class="link-info link-offset-2 link-underline-opacity-25 link-underline-opacity-100-hover"
                  >Intro</a
                >
              </td>
            </tr>
            <tr>
              <th scope="row">PlayList</th>
              <td>
                <a
                  target="”_blank”"
                  id="playLink"
                  href=""
                  class="link-info link-offset-2 link-underline-opacity-25 link-underline-opacity-100-hover"
                  >PlayList</a
                >
              </td>
            </tr>
            <tr>
              <th scope="row">Course Description</th>
              <td id="tdDes"></td>
            </tr>
            <tr>
              <th scope="row">Lesson Content</th>
              <td id="tdCont"></td>
            </tr>
            <tr>
              <th scope="row">last Update</th>
              <td id="tdUp"></td>
            </tr>
          </tbody>
        </table>
      </section>
      <!-- Modal -->
      <div
        class="modal fade"
        id="exampleModal"
        tabindex="-1"
        aria-labelledby="exampleModalLabel"
        aria-hidden="true"
      >
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <h1 class="modal-title fs-5" id="exampleModalLabel">
                Delete Course
              </h1>
              <button
                type="button"
                class="btn-close"
                data-bs-dismiss="modal"
                aria-label="Close"
              ></button>
            </div>
            <div class="modal-body">
              <p class="mb-1">Code:2001</p>
              <p class="mb-1">
                This Action will Delete This course from Database...
              </p>
            </div>
            <div class="modal-footer">
              <button
                type="button"
                class="btn btn-secondary"
                data-bs-dismiss="modal"
              >
                Close
              </button>
              <button type="button" class="btn btn-danger">Delete</button>
            </div>
          </div>
        </div>
      </div>
    </main>
    <script src="./js/bootstrap.bundle.min.js"></script>
    <script src="./js/sidebars.js"></script>
    <script src="./js/home.js"></script>
  </body>
</html>
